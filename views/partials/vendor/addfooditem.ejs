<!-- Modal -->
<div class="modal fade" id="newitempopup" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h2 class="modal-title" id="exampleModalLabel">Create a new food item</h2>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      <form class="addfood">
        Food name:  <input type="text" name="foodname">
      </form>
      <form>
        Price: <input type="text" name="foodprice">
      </form>
      <form>
        Category: <input type="text" name="foodcategory">
      </form>
      <form>
        Type: <input type="text" name="foodtype">
      </form>
      <form>
        Prep Time: <input type="text" name="preptime">
      </form>
      <form>
        Food Image URL: <input type="text" name="imgurl">
      </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary">Create</button>
      </div>
    </div>
  </div>
</div>
